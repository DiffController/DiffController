graph networktopo {
    rankdir = LR;
    node [shape=circle] s1
    node [shape=circle] s2
    node [shape=circle] s3
    node [shape=circle] s4
    node [shape=circle] h1
    node [shape=circle] h2
    node [shape=circle] h3
    node [shape=circle] h4
    
    s1 -- s2 ;
    s1 -- s3 ;
    s3 -- s4 ;
    s1 -- h1 ;
    s2 -- h2 ;
    s3 -- h3 ;
    s4 -- h4 ;
}